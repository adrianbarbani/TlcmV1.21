    <validationRules>
        <fullName>Movil_solo_numeros</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISBLANK( MobilePhone ) , false, NOT(ISNUMBER(MobilePhone)))</errorConditionFormula>
        <errorDisplayField>MobilePhone</errorDisplayField>
        <errorMessage>El telefono solo puede poseer caracteres numericos</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>fecha_de_nacimiento_menor_a_hoy</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISBLANK( Birthdate ) , false, Birthdate &gt; TODAY() )</errorConditionFormula>
        <errorDisplayField>Birthdate</errorDisplayField>
        <errorMessage>La fecha de nacimiento debe ser un valor menor a hoy</errorMessage>
    </validationRules>
    <validationRules>
